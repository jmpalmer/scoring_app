<h1>Listing penalties</h1>

<table>
  <tr>
    <th>Tech foul</th>
    <th>Foul</th>
    <th>Match</th>
    <th>Alliance</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @penalties.each do |penalty| %>
  <tr>
    <td><%= penalty.tech_foul %></td>
    <td><%= penalty.foul %></td>
    <td><%= penalty.match_id %></td>
    <td><%= penalty.alliance_id %></td>
    <td><%= link_to 'Show', penalty %></td>
    <td><%= link_to 'Edit', edit_penalty_path(penalty) %></td>
    <td><%= link_to 'Destroy', penalty, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Penalty', new_penalty_path %>
